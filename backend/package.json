{
  "name": "elite-properties-backend",
  "version": "1.0.0",
  "description": "Backend API for Elite Properties real estate application",
  "main": "src/app.js",
  "scripts": {
    "dev": "nodemon src/app.js",
    "start": "node src/app.js",
    "setup": "node src/scripts/setupDatabase.js && cd src/go && go mod tidy || echo Skipping Go mod tidy (Go not installed or path not found)",
    "setup-dev": "npm install && npm run setup",
    "migrate": "node src/scripts/runMigrations.js",
    "test": "jest",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write src/**/*.js"
  },
  "keywords": [
    "real-estate",
    "api",
    "express",
    "javascript",
    "nodejs"
  ],
  "author": "Elite Properties",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "mysql2": "^3.6.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "eslint": "^8.45.0",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
